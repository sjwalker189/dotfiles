general {
        colors = false
        interval = 1
}

order += "tztime date"
order += "tztime time"


tztime date {
        format = "%a %d %b"
}

tztime time {
        format = "%I:%M %p"
}

volume master {
	format = "VOL: %volume"
	format_muted = "Muted (%volume)"
	device = "default"
}

volume mic {
	format = "MIC: %volume"
	format_muted = "MIC: Off"
	device = "default"
}
